node{
    stage('Pipeline Checkout'){
        git credentialsId: '5353524f-b00d-46e5-8761-3079c34e7525', url: 'https://github.com/vamsik0315/MavenArtifact.git'
    }
    stage('Mvn package'){
    	def mvnHome = tool name: 'maven-3', type: 'maven'
    	def mvnCMD = "${mvnHome}/bin/usr/maven"
    	tool name: 'maven-3', type: 'maven'
    }
}
